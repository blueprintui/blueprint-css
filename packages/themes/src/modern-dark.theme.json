{
  "color-scheme": { "value": "dark" },
  "scale": {
    "interaction-offset": { "value": "1" }
  },
  "text": {
    "color": {
      "100": { "value": "{color.white}" },
      "200": { "value": "{color.gray.800}" },
      "300": { "value": "{color.gray.600}" },
      "400": { "value": "{color.gray.400}" },
      "500": { "value": "{color.gray.200}" },
      "600": { "value": "{color.black}" }
    }
  },
  "object": {
    "border": {
      "color": {
        "100": { "value": "{color.gray.800}" },
        "200": { "value": "{color.gray.700}" },
        "300": { "value": "{color.gray.400}" }
      }
    }
  },
  "layer": {
    "canvas": {
      "background": { "value": "{color.gray.900}" }
    },
    "container": {
      "background": { "value": "{color.gray.700}" }
    },
    "header": {
      "background": { "value": "{color.gray.600}" },
      "color": { "value": "{color.white}" }
    },
    "field": {
      "color": { "value": "{color.white}" },
      "background": {
        "100": { "value": "{color.gray.800}" },
        "200": { "value": "{color.gray.800}" }
      }
    },
    "popover": {
      "background": { "value": "{color.gray.200}" },
      "color": { "value": "{color.gray.900}" },
      "backdrop-background": { "value": "hsla(0, 0%, 0%, 0.6)" }
    }
  },
  "status": {
    "accent": {
      "background": {
        "100": { "value": "{color.blue.100}" },
        "200": { "value": "{color.blue.300}" },
        "300": { "value": "{color.blue.600}" }
      },
      "color": {
        "100": { "value": "{text.color.600}" }
      }
    },
    "success": {
      "background": {
        "100": { "value": "{color.green.100}" },
        "200": { "value": "{color.green.300}" },
        "300": { "value": "{color.green.400}" }
      },
      "color": {
        "100": { "value": "{text.color.600}" }
      }
    },
    "warning": {
      "background": {
        "100": { "value": "{color.yellow.100}" },
        "200": { "value": "{color.yellow.300}" },
        "300": { "value": "{color.yellow.400}" }
      },
      "color": {
        "100": { "value": "{text.color.600}" }
      }
    },
    "danger": {
      "background": {
        "100": { "value": "{color.red.100}" },
        "200": { "value": "{color.red.300}" },
        "300": { "value": "{color.red.400}" }
      },
      "color": {
        "100": { "value": "{text.color.600}" }
      }
    },
    "neutral": {
      "background": {
        "100": { "value": "{color.gray.100}" },
        "200": { "value": "{color.gray.300}" },
        "300": { "value": "{color.gray.400}" }
      },
      "color": {
        "100": { "value": "{text.color.600}" }
      }
    },
    "disabled": {
      "background": {
        "100": { "value": "{color.gray.500}" },
        "200": { "value": "{color.gray.600}" },
        "300": { "value": "{color.gray.600}" }
      },
      "color": {
        "100": { "value": "{text.color.600}" }
      }
    }
  },
  "interaction": {
    "highlight": {
      "background" : { "value": "hsla(211, 100%, 75%, 0.2)" }
    },
    "accent": {
      "background" : { "value": "{status.accent.background.200}" }
    },
    "default": {
      "color": { "value": "{color.gray.100}" }
    },
    "hover": {
      "color": { "value": "{color.gray.200}" }
    },
    "focused": {
      "color": { "value": "{color.gray.700}" }
    },
    "active": {
      "color": { "value": "{color.gray.100}" }
    },
    "selected": {
      "color": { "value": "{color.gray.800}" }
    },
    "disabled": {
      "color": { "value": "{color.gray.400}" }
    }
  }
}
