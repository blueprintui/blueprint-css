{
  "name": "docs",
  "version": "0.0.0",
  "description": "Blueprint UI Layout Documentation",
  "main": "index.js",
  "scripts": {
    "start": "wireit",
    "build": "wireit",
    "start:11ty": "ELEVENTY_ENV=production node -r esm node_modules/.bin/eleventy",
    "start:rollup": "wireit",
    "build:11ty": "wireit",
    "build:rollup": "wireit",
    "debug": "wireit",
    "clean": "wireit",
    "deploy": "wireit"
  },
  "wireit": {
    "start": {
      "dependencies": ["build", "start:11ty", "start:rollup"],
      "service": true
    },
    "start:11ty": {
      "command": "ELEVENTY_ENV=development node -r esm node_modules/.bin/eleventy --watch",
      "service": true
    },
    "start:rollup": {
      "command": "rollup -c -w",
      "service": true
    },
    "build": {
      "command": "NODE_ENV=production",
      "dependencies": ["build:11ty", "build:rollup"]
    },
    "build:11ty": {
      "command": "ELEVENTY_ENV=production node -r esm node_modules/.bin/eleventy"
    },
    "build:rollup": {
      "command": "rollup -c"
    },
    "debug": {
      "command": "DEBUG=* node -r esm node_modules/.bin/eleventy"
    },
    "deploy": {
      "command": "firebase deploy --only hosting:blueprintui-layout",
      "dependencies": ["ci"]
    }
  },
  "homepage": "https://layout.blueprintui.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/blueprintui/layout",
    "https": "https://github.com/blueprintui/layout"
  },
  "author": "Crylan Software",
  "license": "MIT",
  "dependencies": {
    "@blueprintui/components": "0.0.31",
    "@blueprintui/icons": "0.0.20",
    "@blueprintui/layout": "0.0.7",
    "@blueprintui/themes": "0.0.11",
    "@blueprintui/typography": "0.0.13",
    "modern-normalize": "1.1.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "1.0.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "4.2.0",
    "@rollup/plugin-node-resolve": "15.0.1",
    "clean-css": "5.3.1",
    "cross-env": "7.0.3",
    "csso": "5.0.5",
    "esm": "3.2.25",
    "html-minifier": "4.0.0",
    "rollup": "3.4.0",
    "rollup-plugin-browsersync": "1.3.3",
    "rollup-plugin-styles": "4.0.0",
    "rollup-plugin-terser": "7.0.2",
    "terser": "5.16.0"
  }
}
