{
  "name": "@blueprintui/icons-project",
  "version": "0.0.0",
  "type": "module",
  "types": "./index.d.ts",
  "customElements": "./custom-elements.json",
  "license": "MIT",
  "author": "Crylan Software",
  "homepage": "https://blueprintui.dev",
  "description": "Web Component Icon Library for BlueprintUI",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blueprintui/blueprintui.git"
  },
  "bugs": {
    "url": "https://github.com/blueprintui/blueprintui/issues"
  },
  "keywords": [
    "web components",
    "blueprintui"
  ],
  "scripts": {
    "ci": "wireit",
    "start": "wireit",
    "build": "wireit",
    "_build:svg": "wireit",
    "build:watch": "wireit",
    "drafter": "wireit",
    "drafter:watch": "wireit",
    "test": "wireit",
    "test:watch": "wtr --watch",
    "test:unit": "wireit",
    "test:coverage": "wireit",
    "test:a11y": "wireit",
    "test:visual": "wireit",
    "test:visual:update": "wireit",
    "test:api": "wireit",
    "test:api:update": "wireit",
    "test:performance": "wireit",
    "sourcemap": "wireit",
    "publish": "wireit",
    "deploy": "wireit"
  },
  "wireit": {
    "ci": {
      "dependencies": [
        "build",
        "test",
        "test:performance"
      ]
    },
    "start": {
      "dependencies": [
        "build:watch",
        "drafter:watch"
      ]
    },
    "build": {
      "command": "bp build && cpy package.lib.json ./dist/lib --rename package.json",
      "files": [
        "./src/**/*",
        "!./src/**/*.spec.ts",
        "!./src/**/*.performance.ts",
        "./blueprint.config.js",
        "./package.lib.json",
        "./package.json"
      ],
      "output": [
        "./dist/lib"
      ],
      "dependencies": [
        "../themes:build",
        "_build:svg"
      ]
    },
    "build:watch": {
      "command": "bp build --watch",
      "service": true,
      "dependencies": [
        "build"
      ]
    },
    "_build:svg": {
      "command": "node ./svgs.js",
      "files": [
        "./svgs/*.svg",
        "./svgs.js"
      ],
      "output": [
        "./src/shapes/**/*"
      ]
    },
    "drafter": {
      "command": "drafter build",
      "files": [
        "./src/**/*",
        "./blueprint.config.js",
        "./package.lib.json",
        "./package.json"
      ],
      "output": [
        "./dist/drafter"
      ],
      "dependencies": [
        "build"
      ]
    },
    "drafter:watch": {
      "command": "drafter build --watch",
      "service": true,
      "dependencies": [
        "build"
      ]
    },
    "test": {
      "dependencies": [
        "test:api",
        "test:unit",
        "test:visual"
      ]
    },
    "test:watch": {
      "command": "wtr --watch",
      "service": true
    },
    "test:unit": {
      "command": "wtr",
      "files": [
        "./src/**/*",
        "./package.json",
        "./web-dev-server.config.mjs",
        "./web-test-runner.config.mjs"
      ],
      "dependencies": [
        "build"
      ],
      "output": []
    },
    "test:coverage": {
      "command": "wtr --coverage",
      "files": [
        "./src/**/*",
        "./package.json"
      ],
      "output": [
        "./dist/coverage"
      ],
      "dependencies": [
        "build"
      ]
    },
    "test:a11y": {
      "command": "wtr --config ./web-test-runner.a11y.mjs",
      "files": [
        "./src/**/*",
        "./package.json",
        "./web-dev-server.config.mjs"
      ],
      "dependencies": [
        "build"
      ],
      "output": []
    },
    "test:performance": {
      "command": "wtr --config ./web-test-runner.performance.mjs",
      "files": [
        "./src/**/*",
        "./package.json",
        "./web-dev-server.config.mjs",
        "web-test-runner.performance.mjs"
      ],
      "dependencies": [
        "build"
      ],
      "output": []
    },
    "test:visual": {
      "command": "web-test-runner --config ./web-test-runner.visual.mjs",
      "dependencies": [
        "build"
      ],
      "files": [
        "./src/**/*",
        "!./src/**/*.spec.ts",
        "./web-dev-server.config.mjs",
        "./web-test-runner.visual.mjs"
      ],
      "output": []
    },
    "test:visual:update": {
      "command": "web-test-runner --update-visual-baseline --config ./web-test-runner.visual.mjs",
      "dependencies": [
        "build"
      ],
      "files": [
        "./src/**/*",
        "!./src/**/*.spec.ts",
        "./web-dev-server.config.mjs",
        "./web-test-runner.visual.mjs"
      ],
      "output": [
        "screenshots/Chromium/baseline/**/*.png"
      ]
    },
    "test:api": {
      "command": "bp api --test",
      "dependencies": [
        "build"
      ],
      "files": [
        "./src/**/*",
        "./package.json"
      ],
      "output": []
    },
    "test:api:update": {
      "command": "bp api --update",
      "dependencies": [
        "build"
      ]
    },
    "sourcemap": {
      "command": "npx source-map-explorer ./**/*.js",
      "dependencies": [
        "build"
      ]
    },
    "publish": {
      "command": "cd ./dist/lib && npm publish --access public",
      "dependencies": [
        "ci"
      ]
    },
    "deploy": {
      "command": "firebase deploy --only hosting:blueprintui-icons",
      "dependencies": [
        "ci"
      ]
    }
  },
  "files": [
    "*"
  ],
  "sideEffects": [
    "./include.js",
    "./shapes/*.js",
    "./internals/icon.service.js"
  ],
  "exports": {
    "./custom-elements.json": "./custom-elements.json",
    "./shapes/*": "./shapes/*",
    "./include": "./include.js",
    "./include.js": "./include.js",
    "./internals": "./internals/index.js",
    "./internals/index.js": "./internals/index.js",
    ".": "./index.js",
    "./*": "./*/index.js"
  }
}
