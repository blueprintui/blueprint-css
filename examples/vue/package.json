{
  "name": "vue-example",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "wireit",
    "test": "wireit",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "3.2.47"
  },
  "devDependencies": {
    "@types/jsdom": "21.1.0",
    "@types/node": "18.14.2",
    "@vitejs/plugin-vue": "4.0.0",
    "@vue/test-utils": "2.3.0",
    "@vue/tsconfig": "0.1.3",
    "jsdom": "21.1.0",
    "npm-run-all": "4.1.5",
    "typescript": "4.8.4",
    "vite": "4.1.4",
    "vitest": "0.29.1",
    "vue-tsc": "1.2.0"
  },
  "wireit": {
    "build": {
      "command": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false && vite build",
      "dependencies": [
        "../../packages/components:build",
        "../../packages/grid:build",
        "../../packages/icons:build",
        "../../packages/layout:build",
        "../../packages/themes:build",
        "../../packages/typography:build"
      ],
      "files": [
        "src",
        "index.html",
        "tsconfig.json",
        "tsconfig.app.json",
        "vite.config.ts"
      ],
      "output": [
        "dist"
      ]
    },
    "test": {
      "command": "vitest run --silent",
      "dependencies": [
        "build"
      ],
      "files": [
        "src",
        "index.html",
        "tsconfig.json",
        "tsconfig.app.json",
        "vite.config.ts",
        "vitest.config.ts"
      ],
      "output": [
      ]
    }
  }
}
