{
  "name": "@blueprintui/docs",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "ci": "wireit",
    "start": "wireit",
    "build": "wireit",
    "start:11ty": "wireit",
    "start:rollup": "wireit",
    "build:11ty": "wireit",
    "build:rollup": "wireit",
    "debug": "wireit",
    "deploy": "wireit"
  },
  "wireit": {
    "ci": {
      "dependencies": ["build"]
    },
    "start": {
      "dependencies": ["start:11ty", "start:rollup"]
    },
    "build": {
      "command": "NODE_ENV=production node -r esm ../node_modules/.bin/eleventy && rollup -c",
      "dependencies": ["../packages/components:drafter", "../packages/typography:drafter"],
      "files": ["./src/**/*", "./.eleventy.js", "./.eleventyignore", "./rollup.config.mjs", "./rollup-import-assert.mjs"],
      "output": ["./dist"]
    },
    "start:11ty": {
      "command": "ELEVENTY_ENV=development node -r esm ../node_modules/.bin/eleventy --watch",
      "dependencies": ["build"],
      "service": true
    },
    "start:rollup": {
      "command": "rollup -c -w",
      "dependencies": ["build"],
      "service": true
    },
    "debug": {
      "command": "DEBUG=* node -r esm node_modules/.bin/eleventy"
    },
    "deploy": {
      "command": "firebase deploy --only hosting:blueprintui",
      "dependencies": ["ci"]
    }
  },
  "homepage": "https://blueprintui.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/blueprintui/components",
    "https": "https://github.com/blueprintui/components.git"
  },
  "author": "Crylan Software",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blueprintui/components/issues"
  },
  "dependencies": {
    "@blueprintui/components": "0.0.31",
    "@blueprintui/icons": "0.0.20",
    "@blueprintui/layout": "0.0.7",
    "@blueprintui/themes": "0.0.11",
    "@blueprintui/typography": "0.0.13",
    "modern-normalize": "1.1.0",
    "prismjs": "1.29.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "2.0.0-canary.18",
    "@11ty/eleventy-plugin-syntaxhighlight": "4.2.0",
    "@rollup/plugin-alias": "4.0.2",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-terser": "0.1.0",
    "@web/rollup-plugin-copy": "0.3.0",
    "@web/rollup-plugin-html": "1.11.0",
    "acorn-import-assertions": "1.8.0",
    "cross-env": "7.0.3",
    "csso": "5.0.5",
    "decache": "4.6.1",
    "esm": "3.2.25",
    "html-minifier": "4.0.0",
    "markdown-it": "13.0.1",
    "rollup": "3.4.0",
    "rollup-plugin-import-assert": "2.1.3",
    "rollup-plugin-browsersync": "1.3.3",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-styles": "4.0.0",
    "terser": "5.16.0"
  }
}
