{
  "name": "docs",
  "version": "0.0.0",
  "description": "Blueprint UI Layout Documentation",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all clean build:* -p start:*",
    "build": "NODE_ENV=production npm-run-all clean build:*",
    "start:11ty": "ELEVENTY_ENV=development node -r esm node_modules/.bin/eleventy --watch",
    "start:rollup": "rollup -c -w",
    "build:11ty": "ELEVENTY_ENV=production node -r esm node_modules/.bin/eleventy",
    "build:rollup": "rollup -c",
    "debug": "DEBUG=* node -r esm node_modules/.bin/eleventy",
    "clean": "rm -rf dist/ && rm -rf _site/",
    "deploy": "firebase deploy --only hosting:blueprintui-layout"
  },
  "homepage": "https://layout.blueprintui.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/blueprintui/layout",
    "https": "https://github.com/blueprintui/layout"
  },
  "author": "Cory Rylan",
  "license": "MIT",
  "dependencies": {
    "@blueprintui/components": "0.0.19",
    "@blueprintui/icons": "0.0.17",
    "@blueprintui/layout": "0.0.7",
    "@blueprintui/themes": "0.0.7",
    "@blueprintui/typography": "0.0.12",
    "modern-normalize": "1.1.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "1.0.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "4.1.0",
    "@rollup/plugin-node-resolve": "14.1.0",
    "@web/rollup-plugin-copy": "0.3.0",
    "@web/rollup-plugin-html": "1.11.0",
    "clean-css": "5.3.1",
    "cross-env": "7.0.3",
    "csso": "5.0.5",
    "esm": "3.2.25",
    "html-minifier": "4.0.0",
    "npm-run-all": "4.1.5",
    "rollup": "2.79.1",
    "rollup-plugin-browsersync": "1.3.3",
    "rollup-plugin-styles": "4.0.0",
    "rollup-plugin-terser": "7.0.2",
    "terser": "5.15.0"
  }
}
